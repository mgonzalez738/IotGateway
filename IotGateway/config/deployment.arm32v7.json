{
  "modulesContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": "",
            "registryCredentials": {
              "docker": {
                "username": "mgonzalez738",
                "password": "Care12ta",
                "address": "docker.io"
              }
            }
          }
        },
        "systemModules": {
          "edgeAgent": {
            "type": "docker",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-agent:1.0",
              "createOptions": "{}"
            }
          },
          "edgeHub": {
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-hub:1.0",
              "createOptions": "{\"HostConfig\":{\"PortBindings\":{\"5671/tcp\":[{\"HostPort\":\"5671\"}],\"8883/tcp\":[{\"HostPort\":\"8883\"}],\"443/tcp\":[{\"HostPort\":\"443\"}]}}}"
            }
          }
        },
        "modules": {
          "GatewayCore": {
            "version": "1.0.0",
            "type": "docker",
            "env": {
              "storageFolder": {
                "value": "/storage/"
              },
              "configFolder": {
                "value": "/config/"
              }
            },
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mgonzalez738/gatewaycoremodule:0.0.2-arm32v7",
              "createOptions": "{\"HostConfig\":{\"Binds\":[\"/etc/iotedge/GatewayCoreModule/storage/:/storage/\",\"/etc/iotedge/GatewayCoreModule/config/:/config/\"],\"Privileged\":true,\"ExposedPorts\":{\"22/tcp\":{}},\"PortBindings\":{\"22/tcp\":[{\"HostPort\":\"22\"}]}}}"
            }
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "routes": {
          "GatewayCoreModuleToIoTHub": "FROM /messages/modules/GatewayCore/outputs/* INTO $upstream"
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    },
    "GatewayCore": {
      "properties.desired": {
        "DeviceTag": "Raspberry3",
        "DeviceType": "Gateway"
      }
    }
  }
}